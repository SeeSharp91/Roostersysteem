@model Roostersysteem.Models.Vak
@{
    ViewBag.Title = "Vakken";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div class="row">
        <div class="col-lg-12">
            <div class="form-group">
                @Html.LabelFor(model => model._typeVak, htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    Scripting
                    @Html.RadioButtonFor(model => model._typeVak, "Scripting")
                    Modelleren
                    @Html.RadioButtonFor(model => model._typeVak, "Modelleren")
                </div>
            </div>
        </div>
    </div>
</body>
</html>
